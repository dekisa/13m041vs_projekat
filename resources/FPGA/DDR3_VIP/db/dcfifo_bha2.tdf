RETURNS ( bin[2..0]);
FUNCTION a_graycounter_9u6 (aclr, clock, cnt_en)
RETURNS ( q[2..0]);
FUNCTION a_graycounter_5cc (aclr, clock, cnt_en)
RETURNS ( q[2..0]);
